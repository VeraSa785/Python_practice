# Object oriented programming.Class

class BlogPost:
    def __init__(self, user_name, text, number_of_likes):
        self.user_name = user_name
        self.text = text
        self.number_of_likes = number_of_likes

user_number_one = BlogPost(user_name= 'Ban Smith', text= 'some text', number_of_likes = 12)
user_number_two = BlogPost(user_name= 'Stan', text= 'another text', number_of_likes = 33333)
user_number_one.number_of_likes = 12123224

print (user_number_one.number_of_likes)
print (user_number_two.number_of_likes)
